{
  "mcpServers": {
    "playwright": {
      "command": "node",
      "args": [
        "./node_modules/@executeautomation/playwright-mcp-server/dist/index.js"
      ],
      "env": {
        "PLAYWRIGHT_BROWSER": "chromium"
      }
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ]
    }
  }
}